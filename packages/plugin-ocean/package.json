{
  "name": "@elizaos/plugin-ocean",
  "version": "1.0.0",
  "description": "Ocean Protocol integration for ElizaOS - publishes user memories as DataNFTs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.map"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "axios": "^1.7.7",
    "ethers": "^6.13.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@elizaos/plugin-safe": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "eliza",
    "ocean-protocol",
    "datanft",
    "blockchain",
    "ai",
    "memory",
    "data-publishing"
  ],
  "author": "ElizaOS Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaOS/eliza.git",
    "directory": "packages/plugin-ocean"
  }
}